<ol>
{% set tags = [] %}
{% for post in collections.all %}
  {% for tag in post.data.tags %}
    {% if tag not in tags %}
      {% set tags = tags | concat([tag]) %}
    {% endif %}
  {% endfor %}
{% endfor %}
{% for tag in tags %}
  <li>{{ tag }}</li>
{% endfor %}
</ol>